contract Test =
    record state = { transactions : map(int, transaction)}
    
    record transaction = { 
        id            : int,
        confirmations : int,
        method        : method}

    datatype method = Vote | Revoke | Result
          
    public stateful function init() = { transactions = {}}

    public function approve(txId : int) : bool = 
        // let c : option(transaction) = Map.lookup(state.transactions, txId)
        let c = state.transactions[txId]

        true

    private function lookupById(k : int, m) =
        switch(Map.lookup(k, m))
            None    => ""
            Some(x) => x